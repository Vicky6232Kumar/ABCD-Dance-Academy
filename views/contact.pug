extends  base.pug

block scripts 
    script(src='/static/script.js')

block style 
    style
      include ../static/style.css
      include ../static/stylecontact.css

block content 
    h1 Contact us 
    form(action="/contact", method="post", class="myform") 
        input(type="text", class ="myInput", name = "name" placeholder = "Enter your name")
        input(type="number", class ="myInput", name = "phone" placeholder = "Enter your phone number") 
        textarea(name="feedback", class ="myInput", cols="30", rows="10" placeholder = "Give Feedback") 
        button.btn Submit
    
